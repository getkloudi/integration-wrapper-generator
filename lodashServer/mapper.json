[
  {
    "type": "REQUEST",
    "entity": "PULL_REQUEST",
    "integration": "JIRA",
    "method": "ALL|GET|PUT|POST|DELETE"
  },
  {
    "type": "RESPONSE",
    "entity": "PULL_REQUEST",
    "integration": "JIRA",
    "method": "ALL|GET|PUT|POST|DELETE",
    "map": [
      {
        "input": {
          "key": "pr.title",
          "type": "ONE-TO-ONE"
        },
        "output": {
          "key": "title",
          "type": "ONE-TO-ONE"
        }
      },
      {
        "input": {
          "key": "pr.description",
          "type": "ONE-TO-ONE"
        },
        "output": {
          "key": "description",
          "type": "ONE-TO-ONE"
        }
      },
      {
        "input": {
          "key": "toolsRef.name",
          "type": "CONSTANT",
          "value": "BITBUCKET"
        },
        "output": {
          "key": "toolsRef.name",
          "type": "CONSTANT",
          "value": "BITBUCKET"
        }
      },
      {
        "input": {
          "key": "source.commit.hash",
          "type": "FUNCTION",
          "value": "generateFullCommitHash"
        },
        "output": {
          "key": "basheSHA",
          "type": "FUNCTION",
          "value": "generateFullCommitHash"
        }
      },
      {
        "input": {
          "key": "author.display_name",
          "type": "ONE-TO-ONE"
        },
        "output": {
          "key": "author.name",
          "type": "ONE-TO-ONE"
        }
      },
      {
        "input": {
          "key": "participants[${index}].user.display_name",
          "type": "FUNCTION",
          "root": "participants",
          "value": "getPullRequestFromID"
        },
        "output": {
          "key": "reviewers[${index}].name",
          "type": "FUNCTION",
          "root": "reviewers",
          "value": "getPullRequestFromID"
        }
      },
      {
        "input": {
          "key": "participants[${index}].user.id",
          "type": "FUNCTION",
          "root": "participants",
          "value": "getPullRequestFromID"
        },
        "output": {
          "key": "reviewers[${index}].id",
          "type": "FUNCTION",
          "root": "reviewers",
          "value": "getPullRequestFromID"
        }
      }
    ]
  },
  {
    "type": "RESPONSE",
    "entity": "PROJECT",
    "integration": "BITBUCKET",
    "method": "GET",
    "map": [
      {
        "projectName": "name",
        "type": "ONE-TO-ONE"
      },
      {
        "projectId": "full_name",
        "type": "ONE-TO-ONE"
      },
      {
        "projectDescription": "description",
        "type": "ONE-TO-ONE"
      },
      {
        "projectRef": "links.html.href",
        "type": "ONE-TO-ONE"
      },
      {
        "projectIcon": "links.avatar.href",
        "type": "ONE-TO-ONE"
      },
      {
        "organizationId": "full_name",
        "type": "getFirstWordInTitleCase"
      },
      {
        "metrics.key": "Language",
        "type": "CONSTANT"
      },
      {
        "metrics.key": "language",
        "type": "ONE-TO-ONE"
      },
      {
        "updatedOn": "updated_on",
        "type": "ONE-TO-ONE"
      }
    ]
  }
]